<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="HNG: Test letskodeittest" parallel="tests">

    <listeners>
        <listener class-name="com.utilities.reports.testngListener"/>
    </listeners>

    <test name="Login">
        <classes>
            <class name="com.HNG.letskodeit.testSuite.LoginTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Logout">
        <classes>
            <class name="com.HNG.letskodeit.testSuite.LogOutTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Search Courses">
        <classes>
            <class name="com.HNG.letskodeit.testSuite.SearchCoursesTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Detail Courses">
        <classes>
            <class name="com.HNG.letskodeit.testSuite.DetailCourseTest"/>
        </classes>
    </test>

    <test name="CheckOut">
        <classes>
            <class name="com.HNG.letskodeit.testSuite.CheckoutTest"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->
